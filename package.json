{
  "name": "muzo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "rm -rd mini; nodemon --watch \"src/**\" --ext \"ts,json,mts,cts,html,css\" --ignore \"src/**/*.spec.ts\" --exec \"tsc;cp -rd src/static dist;cp -rd src/public dist;node --experimental-modules --es-module-specifier-resolution=node dist/index.js\"",
    "build": "tsc;npx uglifyjs-folder dist --pattern \"**/*.js\" -eo mini -x .js;npx uglifyjs-folder dist --pattern \"**/*.mjs\" -eo mini -x .mjs;npx uglifyjs-folder dist --pattern \"**/*.cjs\" -eo mini -x .cjs;cp -rd src/static mini;cp -rd src/public mini;rm mini/**/*.*ts",
    "start": "node --experimental-modules --es-module-specifier-resolution=node mini/index.js"
  },
  "keywords": [],
  "author": "@Oein",
  "license": "ISC",
  "dependencies": {
    /* better file system */
    "@types/fs-extra": "^9.0.13",
    /* download zip file */
    "archiver": "^5.3.1",
    /* Easy db */
    "better-sqlite3": "^7.6.2",
    /* env */
    "env-paths": "^3.0.0",
    /* env */
    "env-paths-ts": "^1.0.1",
    /* beautiful */
    "eslint": "^8.25.0",
    /* web server */
    "express": "^4.18.2",
    /* better file system */
    "fs-extra": "^10.1.0",
    /* Easy db */
    "quick.db": "^9.0.6",
    /* Typescript > Javascript */
    "typescript": "^4.8.4",
    /* for making session id */
    "uid": "^2.0.1",
    /* for reading config file */
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    /* web server */
    "@types/express": "^4.17.14",
    /* audio file meta data */
    "@types/jsmediatags": "^3.9.3",
    /* beautiful */
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    /* beautiful */
    "@typescript-eslint/parser": "^5.40.0",
    /* audio file meta data */
    "jsmediatags": "^3.9.7",
    /* easy dev */
    "nodemon": "^2.0.20",
    /* build */
    "uglifyjs-folder": "^3.1.4"
  },
  "type": "module"
}
